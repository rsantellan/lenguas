{% extends 'base.html.twig' %}

{% block body %}
<!--page title start-->
<section class="page-title ptb-50">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <ol class="breadcrumb">
                    <li class="active">{% trans %}otros_materiales_titulo{% endtrans %}</li>
                </ol>
            </div>
        </div>
    </div>
</section>
<!--page title end-->
<section class="section-padding">
    <div class="container">

      <div class="mb-80">
          {% trans %}otros_materiales_texto{% endtrans %} 
          <p class="section-sub">
            {% for category in categories %}
          	<a href="{{ path('publicaciones_genericos', {'slug' : category.slug }) }}">
          		<i class="fa fa-caret-right" aria-hidden="true" style="margin-right:5px;"></i>{{ category.longname }}
          	</a>
            {% if not loop.last %}
              </br></br>
            {% endif %}
            {% endfor %}
          </p>
      </div>
    </div><!-- /.container -->
</section>
{% endblock %}